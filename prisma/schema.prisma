generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  telegramId  String   @unique
  ativo       Boolean  @default(false)
  createdAt   DateTime @default(now())
}

model Assinatura {
  id             Int      @id @default(autoincrement())
  telegramId     String
  mpId           String   @unique
  status         String
  startDate      DateTime
  endDate        DateTime
  createdAt      DateTime @default(now())
}
